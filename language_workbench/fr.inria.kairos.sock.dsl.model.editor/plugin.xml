<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated sock -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="fr.inria.kairos.sock.dsl.model.sock.presentation.SockModelWizardID"
            name="%_UI_SockModelWizard_label"
            class="fr.inria.kairos.sock.dsl.model.sock.presentation.SockModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SockModelFile.gif">

         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated sock -->
      <editor
            id="fr.inria.kairos.sock.dsl.model.sock.presentation.SockEditorID"
            name="%_UI_SockEditor_label"
            icon="icons/full/obj16/SockModelFile.gif"
            extensions="sock"
            class="fr.inria.kairos.sock.dsl.model.sock.presentation.SockEditor"
            contributorClass="fr.inria.kairos.sock.dsl.model.sock.presentation.SockActionBarContributor">
      </editor>
